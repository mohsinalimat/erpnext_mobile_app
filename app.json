{
  "expo": {
    "name": "Prime Tech BD",
    "slug": "prime-tech-bd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "paperware.jfmart.site": {
              "NSIncludesSubdomains": true,
              "NSThirdPartyExceptionRequiresForwardSecrecy": false
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.primetechbd.primeerp"
    },
    "android": {
      "package": "com.primetechbd.primeerp",
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "006e0f6c-a16c-4813-a64f-76e8f20a53e1"
      }
    },
    "owner": "primetechbd"
  }
}
